).then(console.log, console.error);\n  });\n\n\n  return (\n    <div className=\"search-params\">\n      <form>\n        {/* Location */}\n        <label htmlFor=\"location\">\n          Location\n          <input\n            id=\"location\"\n            value={location}\n            placeholder=\"Location\"\n            onChange={event => updatedLocation(event.target.value)}\n            onBlur={event => updateLocation(event.target.value)}\n          />\n        </label>\n\n        {/* Animal\n        <label htmlFor=\"animal\">\n          Animal\n          <select\n            id=\"animal\"\n            value={animal}\n            onChange={event => updateAnimal(event.target.value)}\n            onBlur={event => updateAnimal(event.target.value)}\n          >\n            <option>All Animals</option>\n            {ANIMALS.map(animal => (\n              <option \n                key={animal} \n                value={animal}>\n                {animal}\n              </option>\n            ))}\n          </select>\n        </label>\n\n        <label htmlFor='breed'>\n            <select\n            id='breed'\n            value={breed}\n            onChange={event => updateBreed(event.target.value)}\n            onBlur={event => updateBreed(event.target.value)}\n            // if breeds.lenght is 0 you can't click on it. \n            disabled={breeds.length === 0}\n            >\n            \n            <option>All Breeds</option>\n            {breeds.map(breedString => (\n                <option key={breedString} \n                value={breedString}>\n                {breedString}\n                </option>\n            ))}\n\n            </select>\n        </label> */}\n\n        <AnimalDropdown />\n        <BreedDropdown />\n\n\n        <button>Submit</button>\n      </form>\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"error":null,"hash":"fa2d3947287146956dc078dac753b612","cacheData":{"env":{}}}